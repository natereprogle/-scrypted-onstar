{
   "name": "@natereprogle/onstar",
   "description": "A scrypted plugin to allow control over your OnStar enabled vehicles",
   "version": "0.0.21",
   "author": {
      "name": "Nate Reprogle",
      "url": "https://reprogle.org"
   },
   "scripts": {
      "scrypted-setup-project": "scrypted-setup-project",
      "prescrypted-setup-project": "scrypted-package-json",
      "build": "scrypted-webpack",
      "prepublishOnly": "env-cmd -f ./.prodenv scrypted-webpack",
      "prescrypted-vscode-launch": "scrypted-webpack",
      "scrypted-vscode-launch": "scrypted-deploy-debug",
      "scrypted-deploy-debug": "scrypted-deploy-debug",
      "scrypted-debug": "scrypted-debug",
      "scrypted-deploy": "scrypted-deploy",
      "scrypted-readme": "scrypted-readme",
      "scrypted-package-json": "scrypted-package-json"
   },
   "scrypted": {
      "name": "OnStar",
      "type": "DeviceProvider",
      "interfaces": [
         "DeviceProvider",
         "Settings"
      ]
   },
   "homepage": "https://github.com/natereprogle/scrypted-onstar",
   "dependencies": {
      "onstarjs": "^2.3.30",
      "uuid": "^10.0.0"
   },
   "devDependencies": {
      "@scrypted/sdk": "^0.3.0",
      "@types/node": "^20.10.4",
      "env-cmd": "^10.1.0"
   },
   "keywords": [
      "scrypted",
      "plugin",
      "onstar",
      "chevrolet",
      "chevy",
      "gm",
      "gmc",
      "buick",
      "cadillac"
   ]
}
